<div>
  <div>
    <%= form_for @developer, :html => { multipart: true } do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_area :description %>

      <%= f.label :specialties %>
      <%= f.text_field :specialties %>

      <%= f.label :picture %>
      <%= f.file_field :picture %>

      <%= f.submit "Create"%>
    <% end %>
  </div>
</div>